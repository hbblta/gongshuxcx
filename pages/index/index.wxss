/**index.wxss**/
.bannerImg{
  width:750rpx;
  height:400rpx;
}
.bannerImg image{
  width:100%;
  height:400rpx;
}
.allTitle{
  width: 100%;
  height: 393rpx;
  display: flex;
  flex-wrap: wrap;
  align-items: center;
}
.titleList{
  padding-top: 25rpx;
  width: 130rpx;
  height: 170rpx;
  text-align: center;
  margin-left: 30rpx;
  margin-right: 25rpx;
}
.titleList view{
  padding-top: 15rpx;
  font-size:26rpx;
  font-family:PingFangSC;
  font-weight:400;
  color:rgba(85,85,85,1);
}
.titleList image{
  width: 79rpx;
  height: 79rpx;
}
.hr{
  width:100%;
  height:15rpx;
  background:rgba(239,239,244,1);
}
.homeland{
  height: 93rpx;
  line-height: 93rpx;
}
.homeland text:nth-child(1){
  display: inline-block;
  width:6rpx;
  height:26rpx;
  background:#C93E3E;
  margin-left: 28rpx;
  margin-right: 20rpx;
}
.homeland text:nth-child(2){
  font-size:32rpx;
  font-family:PingFangSC;
  font-weight:500;
  color:rgba(0,0,0,1);
}
.homelandImg{
  width: 100%;
  height: 360rpx;
}
.homelandImg image{
  width: 100%;
  height: 360rpx;
}