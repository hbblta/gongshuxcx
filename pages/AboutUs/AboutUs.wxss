/* pages/AboutUs/AboutUs.wxss */
page{
  background: #EFEFF4;
}
.listAll{
  padding-left:30rpx ;
}
.aboutMsg{
  margin-top: 53rpx;
  height: 614rpx;
  border-bottom: 1rpx solid rgba(153,153,153,1);
}
.aboutMsg image{
  width: 100%;
  height:482rpx;
  width: 95%;
}
.titleText{
  font-size:32rpx;
  font-family:PingFangSC;
  font-weight:500;
  width: 95%;
  color:rgba(17,17,17,1);
  padding-top: 18rpx;
}
.bodyText{
  font-size:24rpx;
  font-family:PingFangSC;
  font-weight:400;
  width: 95%;
  color:rgba(153,153,153,1);
  padding-top: 19rpx;
  overflow: hidden;
  text-overflow:ellipsis;
  display:-webkit-box;
  -webkit-box-orient:vertical;
  -webkit-line-clamp:1;
}
.title{
  background: white;
  width:100%;
  height:108rpx;
  display: flex;
  justify-content: space-around;
  }
  .title text{
    text-align: center;
    position: relative;
    display: inline-block;
    width:124rpx;
    font-size:30rpx;
    font-family:PingFangSC;
    font-weight:500;
    line-height: 100rpx;
  }
  .titleChange{
    color: rgba(201,62,62,1);
  }
  .titleChange ::after{
    position: absolute;
    content: '';
    width:126rpx;
    height:6rpx;
    bottom: 15rpx;
    left: 0;
    background:rgba(201,62,62,1);
  }