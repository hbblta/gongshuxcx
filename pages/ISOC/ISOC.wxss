/* pages/ISOC/ISOC.wxss */
@import '../../wxParse/wxParse.wxss';
.title{
width:100%;
height:108rpx;
display: flex;
justify-content: space-around;
}
.title text{
  position: relative;
  display: inline-block;
  width:124rpx;
  font-size:30rpx;
  font-family:PingFangSC;
  font-weight:500;
  line-height: 100rpx;
}
.titleChange{
  color: rgba(201,62,62,1);
}
.titleChange ::after{
  position: absolute;
  content: '';
  width:126rpx;
  height:6rpx;
  bottom: 15rpx;
  left: 0;
  background:rgba(201,62,62,1);
}
.hr{
width: 100%;
height: 26rpx;
background: #EEF0F6;
}
.msgData{
  padding: 20rpx;
  font-size:28rpx;
  font-family:PingFangSC;
  font-weight:400;
  color:rgba(14,14,14,1);
}